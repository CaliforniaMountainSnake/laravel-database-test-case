{
    "name": "californiamountainsnake/laravel-database-test-case",
    "type": "library",
    "description": "This is the class intended for the testing the anything that need the database connection with Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "CaliforniaMountainSnake",
            "email": "CaliforniaMountainSnake1@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-pdo": "*",
        "laravel/framework": "~5.8"
    },
    "require-dev": {
        "ext-dom": "*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "CaliforniaMountainSnake\\LaravelDatabaseTestCase\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "\"vendor/bin/phpunit\""
        ]
    }
}
